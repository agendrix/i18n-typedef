{
  "name": "i18n-typedef",
  "version": "1.2.0",
  "description": "Convert I18n translation files to TypeScript definitions",
  "author": "Agendrix",
  "repository": "github:agendrix/i18n-typedef",
  "keywords": [
    "I18n",
    "yaml",
    "TypeScript",
    "CLI"
  ],
  "bin": {
    "i18n-typedef": "./bin/i18n-typedef"
  },
  "license": "Apache-2.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.release.json",
